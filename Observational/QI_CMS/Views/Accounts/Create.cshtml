@using ES_CapDien.Models
@using WebMatrix.WebData;
@model UserProfileModel
@{
    ViewBag.Title = "Thêm mới tài khoản";
}
@section PageScripts{
    <script type="text/javascript">
        var runSelect2 = function () {
            $(".search-select").select2({
                allowClear: true
            });
        };
        runSelect2();
    </script>
}
<div class="col-md-12">
    <div class="page-header">
        <h1 class="titlepageas"><a class="homcomback" href="/DashBoard/DashBoardManager"><img src="~/Images/home.png" /></a>@ViewBag.Title</h1>
    </div>
    @Html.Partial("~/Views/Shared/_MessagePartial.cshtml")
    <div class="panel panel-default">
        <div class="panel-heading">
            <i class="fa fa-external-link-square"></i><b>@ViewBag.Title</b>
        </div>
        <div class="panel-body">
            @using (Html.BeginForm("Create", "Accounts", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary()
                @Html.Partial("_UserProfilePartial")
            }
        </div>
    </div>
</div>
